<?xml version="1.0" encoding="utf-8"?>
<animated-vector xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:aapt="http://schemas.android.com/aapt"
    android:drawable="@drawable/vector_searchback_search">

    <target android:name="stem">
        <aapt:attr name="android:animation">
            <set android:ordering="together">
                <objectAnimator
                    android:duration="@integer/searchback_duration_75"
                    android:propertyName="trimPathStart"
                    android:startOffset="@integer/searchback_duration_25"
                    android:valueFrom="0"
                    android:valueTo=".75" />
                <objectAnimator
                    android:duration="@integer/searchback_duration_75"
                    android:propertyName="trimPathEnd"
                    android:startOffset="@integer/searchback_duration_25"
                    android:valueFrom="0.185"
                    android:valueTo="1" />
            </set>
        </aapt:attr>
    </target>

    <target android:name="circle">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="@integer/searchback_duration_50"
                android:propertyName="trimPathEnd"
                android:valueFrom="1"
                android:valueTo="0" />
        </aapt:attr>
    </target>

    <target android:name="arrow">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="@integer/searchback_duration_50"
                android:propertyName="translateX"
                android:startOffset="@integer/searchback_duration_50"
                android:valueFrom="@integer/searchback_arrow_translationX"
                android:valueTo="0" />
        </aapt:attr>
    </target>

    <target android:name="arrow_top">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="@integer/searchback_duration_50"
                android:propertyName="trimPathEnd"
                android:startOffset="@integer/searchback_duration_50"
                android:valueFrom="0"
                android:valueTo="1" />
        </aapt:attr>
    </target>

    <target android:name="arrow_bottom">
        <aapt:attr name="android:animation">
            <objectAnimator
                android:duration="@integer/searchback_duration_50"
                android:propertyName="trimPathEnd"
                android:startOffset="@integer/searchback_duration_50"
                android:valueFrom="0"
                android:valueTo="1" />
        </aapt:attr>
    </target>
</animated-vector>